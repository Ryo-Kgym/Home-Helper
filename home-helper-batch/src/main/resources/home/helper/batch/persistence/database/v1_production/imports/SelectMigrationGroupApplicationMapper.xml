<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="home.helper.batch.persistence.database.v1_production.imports.SelectMigrationGroupApplicationMapper">
    <select id="selectMigrationGroupApplication"
            resultType="home.helper.batch.dto.v1.imports.ImportMigrationGroupApplicationOutput">
        select *
        from public.group_application
        order by
        group_id,
        application_id
    </select>
</mapper>