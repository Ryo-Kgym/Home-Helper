<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="context1">
        <plugin type="com.itfsw.mybatis.generator.plugins.LombokPlugin">
            <property name="@Data" value="true"/>
            <property name="@Builder" value="true"/>
            <property name="@SuperBuilder" value="false"/>
            <property name="@Value" value="false"/>
            <property name="@AllArgsConstructor" value="true"/>
            <property name="@NoArgsConstructor" value="true"/>
            <property name="@Accessors(chain = true)" value="false"/>
            <property name="supportSuperBuilderForIdea" value="false"/>
        </plugin>

        <plugin type="com.itfsw.mybatis.generator.plugins.MapperAnnotationPlugin">
            <property name="@Mapper" value="true"/>
            <property name="@Repository" value="false"/>
        </plugin>

        <commentGenerator>
            <!-- 生成される Java ファイルのコメントに日付を付与しない -->
            <property name="suppressAllComments" value="false"/>
            <property name="suppressDate" value="true"/>
        </commentGenerator>

        <jdbcConnection
                connectionURL="jdbc:postgresql://localhost:5432/home_helper_db"
                driverClass="org.postgresql.Driver"
                userId="postgres"
                password="postgres"
        />

        <javaTypeResolver>
            <property name="useJSR310Types" value="true"/>
            <property name="forceBigDecimals" value="true"/>
        </javaTypeResolver>

        <javaModelGenerator
                targetPackage="home.helper.batch.persistence.database.v1.table"
                targetProject="src/main/java">
            <property name="enableSubPackages" value="false"/>
            <property name="trimStrings" value="false"/>
        </javaModelGenerator>
        <sqlMapGenerator
                targetPackage="home.helper.batch.persistence.database.v1.table"
                targetProject="src/main/resources">
            <property name="enableSubPackages" value="false"/>
            <property name="trimStrings" value="false"/>
        </sqlMapGenerator>

        <javaClientGenerator
                type="XMLMAPPER"
                targetPackage="home.helper.batch.persistence.database.v1.table"
                targetProject="src/main/java">
            <property name="enableSubPackages" value="false"/>
            <property name="trimStrings" value="false"/>
        </javaClientGenerator>

        <!--        <table schema="public" tableName="account" mapperName="DbV1AccountMapper" domainObjectName="DbV1Account"-->
        <!--               enableCountByExample="false" enableDeleteByExample="false" enableSelectByExample="false"-->
        <!--               enableUpdateByExample="false" modelType="flat">-->
        <!--            <columnOverride column="account_id" property="accountId" javaType="java.lang.String" jdbcType="VARCHAR"/>-->
        <!--            <columnOverride column="group_id" property="groupId" javaType="java.lang.String" jdbcType="VARCHAR"/>-->
        <!--        </table>-->
        <table schema="public" tableName="affiliation" mapperName="DbV1AffiliationMapper"
               domainObjectName="DbV1Affiliation"
               enableCountByExample="false" enableDeleteByExample="false" enableSelectByExample="false"
               enableUpdateByExample="false" modelType="flat">
            <columnOverride column="affiliation_id" property="affiliationId" javaType="java.lang.String"
                            jdbcType="VARCHAR"/>
            <columnOverride column="user_id" property="userId" javaType="java.lang.String" jdbcType="VARCHAR"/>
            <columnOverride column="group_id" property="groupId" javaType="java.lang.String" jdbcType="VARCHAR"/>
            <columnOverride column="group_role_id" property="groupRoleId" javaType="java.lang.String"
                            jdbcType="VARCHAR"/>
        </table>
        <table schema="public" tableName="application" mapperName="DbV1ApplicationMapper"
               domainObjectName="DbV1Application"
               enableCountByExample="false" enableDeleteByExample="false" enableSelectByExample="false"
               enableUpdateByExample="false" modelType="flat">
            <columnOverride column="application_id" property="applicationId" javaType="java.lang.String"
                            jdbcType="VARCHAR"/>
        </table>
        <table schema="public" tableName="category" mapperName="DbV1CategoryMapper" domainObjectName="DbV1Category"
               enableCountByExample="false" enableDeleteByExample="false" enableSelectByExample="false"
               enableUpdateByExample="false" modelType="flat">
            <columnOverride column="category_id" property="categoryId" javaType="java.lang.String" jdbcType="VARCHAR"/>
            <columnOverride column="genre_id" property="genreId" javaType="java.lang.String" jdbcType="VARCHAR"/>
            <columnOverride column="group_id" property="groupId" javaType="java.lang.String" jdbcType="VARCHAR"/>
        </table>
        <table schema="public" tableName="credit_card_detail" mapperName="DbV1CreditCardDetailMapper"
               domainObjectName="DbV1CreditCardDetail"
               enableCountByExample="false" enableDeleteByExample="false" enableSelectByExample="false"
               enableUpdateByExample="false" modelType="flat">
            <columnOverride column="id" property="id" javaType="java.lang.String" jdbcType="VARCHAR"/>
            <columnOverride column="category_id" property="categoryId" javaType="java.lang.String" jdbcType="VARCHAR"/>
            <columnOverride column="genre_id" property="genreId" javaType="java.lang.String" jdbcType="VARCHAR"/>
            <columnOverride column="summary_id" property="summaryId" javaType="java.lang.String" jdbcType="VARCHAR"/>
            <columnOverride column="user_id" property="userId" javaType="java.lang.String" jdbcType="VARCHAR"/>
            <columnOverride column="group_id" property="groupId" javaType="java.lang.String" jdbcType="VARCHAR"/>
            <columnOverride column="genre_id" property="genreId" javaType="java.lang.String" jdbcType="VARCHAR"/>
        </table>
        <!--        </table>-->
        <!--        <table schema="public" tableName="user" mapperName="DbV1UserMapper" domainObjectName="DbV1User"-->
        <!--               enableCountByExample="false" enableDeleteByExample="false" enableSelectByExample="false"-->
        <!--               enableUpdateByExample="false" modelType="flat">-->
        <!--            <columnOverride column="user_id" property="userId" javaType="java.lang.String" jdbcType="VARCHAR"/>-->
    </context>
</generatorConfiguration>