query {
    helpPointEarnedAchievement(param: {
        userId: "u1"
    }) {
        id
        userId
        earnedDate
        earnedPoint
        helpPointEarnedDetailByEarnedAchievementId {
            earnedAchievementId
            helpItemByEarnedDetail {
                id
                name
                point
                memo
            }
            helpItemCount
            helpItemTotalPoint
        }
    }
}